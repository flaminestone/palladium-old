<p id="notice"><%= notice %></p>

<h1>Listing Result Sets</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Version</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @result_sets.each do |result_set| %>
      <tr>
        <td><%= result_set.status %></td>
        <td><%= result_set.version %></td>
        <td><%= link_to 'Show', result_set %></td>
        <td><%= link_to 'Edit', edit_result_set_path(result_set) %></td>
        <td><%= link_to 'Destroy', result_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result set', new_result_set_path %>
