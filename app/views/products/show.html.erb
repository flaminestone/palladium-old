<div class="container-fluid">
  <div class="row">
    <div class="col-xs-2 sidebar-underspase">
      <div class="create-new-plan-btn">
        <span class="create-new">Create new plan  </span>
      </div>
    </div>
    <div class="col-xs-9 plan-element-underspase">
      <div class="plans-title">
        <span>Plans</span>
      </div>
      <div class="plan-list-box">
        <table class="table table-hover">
          <tbody>
          <% @product.plans.each do |current_plan| %>
              <tr>
                <td><a class="plan-name-link link" href="<%= product_plan_path(@product, current_plan) %>"><%= current_plan.name %></a></td>
                <td><%= current_plan.updated_at %></td>
              </tr>
          <% end  %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
</div>






